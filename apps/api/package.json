{
  "name": "@starter/api",
  "version": "0.0.0",
  "private": true,
  "main": "./src/server.ts",
  "license": "MIT",
  "scripts": {
    "db:migrate": "npm run env:load prisma migrate dev",
    "db:studio": "npm run env:load prisma studio",
    "dev": "npm run env:load tsx watch ./src/server.ts",
    "build": "tsup src --out-dir build",
    "start": "node build/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "env:load": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@prisma/client": "5.18.0",
    "@starter/env": "*"
  },
  "devDependencies": {
    "@starter/eslint-config": "*",
    "@starter/prettier": "*",
    "@starter/tsconfig": "*",
    "@types/node": "20.13.0",
    "@vitest/coverage-v8": "2.0.5",
    "@vitest/ui": "2.0.5",
    "dotenv-cli": "7.4.2",
    "prisma": "5.18.0",
    "tsup": "8.2.4",
    "tsx": "4.16.5",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.0.5"
  },
  "eslintConfig": {
    "extends": [
      "@starter/eslint-config/node"
    ]
  },
  "prettier": "@starter/prettier"
}
